{
  "name": "pizza",
  "displayName": "Pizza",
  "description": "Order pizza without ever leaving VSCode",
  "version": "1.0.1",
  "publisher": "WebDevSimplified",
  "repository": {
    "url": "https://github.com/WebDevSimplified/vscode-extension-pizza"
  },
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pizza.orderPizza"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Pizza",
      "properties": {
        "pizza.firstName": {
          "type": "string",
          "description": "Your first name"
        },
        "pizza.lastName": {
          "type": "string",
          "description": "Your last name"
        },
        "pizza.phoneNumber": {
          "type": "string",
          "description": "Your phone number"
        },
        "pizza.email": {
          "type": "string",
          "description": "Your email"
        },
        "pizza.address": {
          "type": "string",
          "description": "Your address"
        }
      }
    },
    "commands": [
      {
        "command": "pizza.orderPizza",
        "title": "EMERGENCY PIZZA"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-proposal-private-methods": "7.13.0",
    "@babel/plugin-syntax-class-properties": "7.12.13",
    "@babel/preset-env": "7.13.15",
    "@types/glob": "7.1.3",
    "@types/mocha": "8.2.2",
    "@types/node": "12.20.7",
    "@types/vscode": "1.55.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-loader": "8.2.2",
    "eslint": "7.24.0",
    "glob": "7.1.6",
    "mocha": "8.2.1",
    "ts-loader": "8.1.0",
    "typescript": "4.2.4",
    "vscode-test": "1.5.0",
    "webpack": "5.32.0",
    "webpack-cli": "4.6.0"
  },
  "dependencies": {
    "dominos": "^3.3.0"
  }
}
